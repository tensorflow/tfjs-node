{
  "name": "tfjs-node",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "gypfile": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tensorflow/tfjs-node.git"
  },
  "scripts": {
    "prep": "cd node_modules/@tensorflow/tfjs-core && yarn && yarn build",
    "build": "tsc",
    "clean": "rm -rf deps/tensorflow/*",
    "format": "clang-format -i -style=Google binding/*.cc binding/*.h",
    "lint": "tslint -p . -t verbose",
    "test": "ts-node src/run_tests.ts"
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "tensorflow/tfjs-core",
    "@types/bindings": "~1.3.0",
    "@types/jasmine": "~2.8.6",
    "@types/node": "~9.6.2",
    "clang-format": "~1.2.2",
    "jasmine": "~3.1.0",
    "ts-node": "~5.0.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  },
  "dependencies": {
    "bindings": "~1.3.0"
  }
}
